{
  "name": "@smlee/free-tunnel-server",
  "version": "0.1.1",
  "description": "Free ngrok alternative: WebSocket reverse-tunnel server with HTTP routing and subdomains.",
  "type": "commonjs",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "bin": {
    "free-tunnel-server": "dist/cli.js"
  },
  "keywords": [
    "free",
    "free tunnel",
    "free ngrok",
    "free ngrok alternative",
    "ngrok alternative",
    "localtunnel alternative",
    "reverse tunnel server",
    "reverse proxy",
    "http tunnel",
    "websocket",
    "ws proxy",
    "subdomain routing",
    "tunneling",
    "proxy",
    "self-hosted",
    "devops",
    "ingress"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smlee/free-tunnel.git"
  },
  "homepage": "https://github.com/smlee/free-tunnel#readme",
  "bugs": {
    "url": "https://github.com/smlee/free-tunnel/issues"
  },
  "author": "Sangmin Lee",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf dist *.tsbuildinfo",
    "build": "tsc -b",
    "start": "node dist/cli.js",
    "dev": "ts-node-dev --respawn --transpile-only src/cli.ts",
    "prepublishOnly": "npm run clean && npm run build",
    "publish:public": "npm publish --access public",
    "release": "npm run clean && npm run build && npm run publish:public"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "license": "PolyForm-Noncommercial-1.0.0",
  "dependencies": {
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "uuid": "^9.0.1",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
